{
  "name": "voidjs-com",
  "private": true,
  "version": "1.0.0",
  "description": "The site of the Void NodeJS module",
  "main": "gulpfile.js",
  "scripts": {
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edj-boston/voidjs-com"
  },
  "keywords": [
    "void"
  ],
  "author": "Eric Johnson <edj.boston@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edj-boston/voidjs-com/issues"
  },
  "homepage": "https://voidjs.com",
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "bootstrap": "^3.3.6",
    "connect-fonts-sourcecodepro": "0.0.3",
    "del": "^2.0.2",
    "express": "^4.13.3",
    "font-awesome": "^4.5.0",
    "gulp": "^3.9.0",
    "gulp-add-src": "^0.2.0",
    "gulp-awspublish": "^3.0.1",
    "gulp-babel": "^6.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.0.1",
    "gulp-david": "^0.4.1",
    "gulp-depcheck": "^1.1.0",
    "gulp-eslint": "^1.1.1",
    "gulp-gzip": "^1.2.0",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^2.0.0",
    "gulp-less": "^3.0.5",
    "gulp-load-plugins": "^1.2.0",
    "gulp-mocha": "^2.1.3",
    "gulp-sequence": "^0.4.4",
    "gulp-tap": "^0.1.3",
    "gulp-uglify": "^1.4.2",
    "gulp-util": "^3.0.7",
    "handlebars": "^4.0.5",
    "handlebars-layouts": "^3.1.3",
    "jquery": "^2.1.4",
    "jsdom": "^8.0.1",
    "marked": "^0.3.5",
    "mocha": "^2.3.3",
    "moment": "^2.10.6",
    "npm-font-open-sans": "0.0.3",
    "should": "^8.0.2",
    "void": "^1.0.0",
    "yargs": "^3.31.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true,
      "mocha": true
    },
    "globals": {
      "$": true,
      "window": true,
      "document": true,
      "ga": true
    },
    "rules": {
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "arrow-parens": [
        2,
        "as-needed"
      ],
      "arrow-spacing": 2,
      "callback-return": 2,
      "comma-dangle": 2,
      "comma-spacing": 2,
      "comma-style": 2,
      "computed-property-spacing": 2,
      "consistent-this": [
        2,
        "self"
      ],
      "brace-style": 2,
      "camelcase": [
        2,
        {
          "properties": "always"
        }
      ],
      "curly": [
        2,
        "multi-line",
        "consistent"
      ],
      "global-require": 2,
      "eol-last": 2,
      "handle-callback-err": 2,
      "indent": 2,
      "key-spacing": [
        2,
        {
          "align": "colon",
          "beforeColon": true,
          "afterColon": true
        }
      ],
      "linebreak-style": 2,
      "lines-around-comment": 2,
      "no-array-constructor": 2,
      "no-const-assign": 2,
      "no-lonely-if": 2,
      "no-mixed-spaces-and-tabs": 2,
      "no-multiple-empty-lines": [
        2,
        {
          "max": 2
        }
      ],
      "no-negated-condition": 2,
      "no-path-concat": 2,
      "no-restricted-modules": [
        2,
        "assert",
        "os"
      ],
      "no-shadow": 2,
      "no-spaced-func": 2,
      "no-trailing-spaces": 2,
      "no-unused-vars": 2,
      "no-unneeded-ternary": 2,
      "object-curly-spacing": [
        2,
        "always"
      ],
      "no-use-before-define": 2,
      "no-var": 2,
      "padded-blocks": [
        2,
        "never"
      ],
      "prefer-arrow-callback": 2,
      "prefer-const": 1,
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "semi": 2,
      "semi-spacing": 2,
      "space-after-keywords": 2,
      "space-before-keywords": 2,
      "space-before-blocks": 2,
      "space-before-function-paren": 2,
      "sort-vars": 2,
      "space-in-parens": 2,
      "space-infix-ops": 2,
      "space-return-throw-case": 2,
      "space-unary-ops": 2,
      "spaced-comment": [
        2,
        "always",
        {
          "markers": [
            "!"
          ]
        }
      ],
      "no-arrow-condition": 2,
      "no-class-assign": 2,
      "no-dupe-class-members": 2,
      "no-this-before-super": 2,
      "object-shorthand": 2,
      "prefer-reflect": 2,
      "prefer-spread": 2,
      "prefer-template": 2,
      "require-yield": 2
    }
  }
}